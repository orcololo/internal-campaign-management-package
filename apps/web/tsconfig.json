{
  "extends": "../../packages/config-typescript/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "strictNullChecks": false,
    "noImplicitAny": false,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "**/*.mts"
  ],
  "exclude": [
    "node_modules"
  ]
}